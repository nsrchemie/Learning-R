    "x <- father.son$fheight\n",
    "ps <- ( seq(0,99) + 0.5 )/100\n",
    "qs <- quantile(x, ps)\n",
    "normalqs <- qnorm(ps, mean(x), popsd(x))\n",
    "plot(normalqs, qs, xlab-)"
